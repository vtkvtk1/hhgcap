                    <table id="table">
                        <thead>
                            <tr>
                                <td><b class="name">Amount</b></td>
                                <td><b class="name">Payment mode</b></td>
                                <td><b class="name">Status</b></td>
                                <td><b class="name">Date made</b></td>
                            </tr>
                        </thead>
                        <tbody>
                            <% deposits.forEach((deposit)=>{ %>
                                <tr>
                                    <td><b><%=`$${deposit.amount}`%></b></td>
                                    <td>
                                        <%=`${deposit.paymentMode}`%>
                                    </td>
                                    <td>
                                        <%=`${deposit.status}`%>
                                    </td>
                                    <td>
                                        <%= deposit.createdAt.toString().slice(0,15) %>
                                    </td>
                                </tr>
                                <% }) %>
                        </tbody>
                    </table>
                    <% } else { %>
                        <div style="background-color: rgb(255, 89, 89); padding: 10px;">
